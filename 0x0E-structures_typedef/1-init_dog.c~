#include <stdio.h>
#include "dog.h"


/**
 * init_dog - function that initialize a variable of type struct dog
 *
 * @d: variable of type struct dog *
 * @name: name of the dog
 * @age: age of the dog
 * @owner: owner of the dog
 *
 * Description: function that initialize a viable of type struct dog
 *
 * Return: nothing
 */

void init_dog(strut dog *d, char *name, float age, char *owner)
{
	d->name = name;
	d->age = age;
	d->owner = owner;
}
