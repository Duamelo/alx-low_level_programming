#include <stdio.h>
#include "main.h"

/**
 * print_rev - print a string
 *
 * @s: string parameter to be printed
 *
 * Description: prints a string, in reverse, followed by a new line
 *
 * Return: Nothing
 */
void print_rev(char *s)
{
	int i;

	i = strlen(s);
	while (i >= 0)
	{
		_putchar(s[i]);
		i--;
	}
	_putchar('\n');
}
